//////////////////////////////////????????????????		
///////////////////////????????????????
///////////////////////????????????????
function funk_wstaw($nazwa_otrz,$lub, $val_min, $val_max,$val_len,$size_v,$wid_v,$ali){
		

//if (isset($_POST['$nazwa_otrz'])) echo 'OTRZYMAL='.$_POST['$nazwa_otrz'];

	$wstaw='<input type="number" name="'.$nazwa_otrz.'" ';



if(isset($_POST['praca_g'])&&(isset($_POST['praca_pod']))&&(isset($_POST['przerw_g']))&&(isset($_POST['przerw_pod_s']))&&(isset($_POST['przerw_pod_m']))) 
{
	$flaga=1;
	$przer_pod_1=$_POST['przerw_pod_s']+$_POST['przerw_pod_m']*60;
	$g_1h=round($_POST['praca_g']/(($_POST['praca_g']+$_POST['przerw_g'])/60),1);
	$pod_1h=round($_POST['praca_pod']/(($_POST['praca_pod']+$przer_pod_1)/60),1);
	$roznica_1h=$g_1h-$pod_1h;
	$proc_1h=round(100*$g_1h/$pod_1h,0);

	//echo $g_1h.'jest'.$pod_1h;
	
}
switch ($nazwa_otrz) {
		case 'g_1h': $wstaw=$wstaw.'value='.$g_1h.' ';break;
		case 'pod_1h': $wstaw=$wstaw.'value='.$pod_1h.' ';break;
		case 'roznica_1h': $wstaw=$wstaw.'value='.$roznica_1h.' ';break;
		case 'proc_1h': $wstaw=$wstaw.'value='.$proc_1h.' %';break;
		
		default:{
	
if (isset($_POST[$nazwa_otrz])) $wstaw=$wstaw.'value='.$_POST[$nazwa_otrz].' ' ;else 
	$wstaw=$wstaw.'value='.$lub.$_POST[$nazwa_otrz].' ';
		}
		} 
//if (isset($g_1h))||(isset($g_1h))||(isset($pod_1h))||(isset($g_1h))||(isset($g_1h)) {

$wstaw=$wstaw.' min="'.$val_min.'" max="'.$val_max.' "  size='.$size_v.' maxlength="'.$val_len.' " style=" width:'.$wid_v.'px; text-align: '.$ali.';"'; 
	$wst_1="	<br/></td> \n";
	
	switch ($nazwa_otrz) {
		case 'g_1h': 		$wstaw=$wstaw.' readonly >'.$wst_1;break;
		case 'pod_1h': 		$wstaw=$wstaw.' readonly >'.$wst_1;break;
		case 'roznica_1h': 	$wstaw=$wstaw.' readonly >'.$wst_1;break;
		case 'proc_1h': 	$wstaw=$wstaw.' readonly >%'.$wst_1;break;
		case 'przerw_pod_m': $wstaw=$wstaw.'>[min]'.$wst_1;break;
		case 'przerw_pod_s': $wstaw=$wstaw.' >[sek]'.$wst_1;break;
		
		default : $wstaw=$wstaw.'>'.$wst_1;break;
	}

	echo ''.$wstaw.'' ;
}

////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////		($nazwa_otrz,$lub, $val_min, $val_max,$val_len,$size_v,$wid_v,$ali)

//	echo ' <br> WYNIKI:'.$_POST['praca_g'].'++'.$_POST['przerw_g'].'++'.$_POST['praca_pod'].'++'.$_POST['przerw_pod_m'].'++'.$_POST['przerw_pod_s'].'++  <br>';
//	echo $g_1h.'jest'.$pod_1h.'  <br>';
//		<tr id="t01"><td id="t01">	TRYB PODTRZYMANIA czas przerwy [sek]:	</td><td><?php funk_wstaw('przerw_pod_s',0,0,2000,4,10,60,'center'); 
//<tr id="t01"><td id="t01">	 jak dlugo trwa [min]:	</td><td><?php funk_wstaw('ile_pod',30,0,1000,6,10,60,'center');
		?>
<table cellpadding=5 bgcolor="#1E90FF"   width=350>
		<tr  height=90><th  colspan="2">	<h2><b>Kalkulator OSZCZĘDNOŚCI <br>TRYB PODTRZYMANIA</b></h2>	</th></tr>
		<tr><td>		
		<center>
		<form action="#podtrzymanie" method="post"> 	
		<table>
		<tr id="t01" height=10 ><th COLSPAN=3 style=" background-color:#E0FFFF;  text-align:center; ">WPISZ DANE </th></tr>
		<tr id="t01"><td rowspan=2 style=" background-color:#40E0D0; "><b>TRYB GRZANIA</b></td>
			<td id="t01" align="left" >	 czas pracy podajnika [sek]:	</td><td  width=130>	 <?php funk_wstaw('praca_g',5,1,500,3,10,60,'center');?> </tr> 
		
		<tr id="t01"><td id="t01">	 czas przerwy podajnika[sek]:	</td><td>	 <?php funk_wstaw('przerw_g',40,0,500,3,8,60,'center');?> 
		<tr id="t01"><td rowspan=2 style=" background-color:#40E0D0; "><b>TRYB PODTRZYMANIA</b></td>
